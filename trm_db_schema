-- ============================================================================
-- TASC-RestoreMed Task 4.1 Database Schema
-- Project Identification, Categorisation and Clustering
-- MySQL 5.7.4
-- ============================================================================

-- Drop existing tables if they exist (in reverse order of dependencies)
DROP TABLE IF EXISTS search_logs;
DROP TABLE IF EXISTS project_synergies;
DROP TABLE IF EXISTS synergy_projects;
DROP TABLE IF EXISTS synergies;
DROP TABLE IF EXISTS project_keywords;
DROP TABLE IF EXISTS keywords;
DROP TABLE IF EXISTS project_clusters;
DROP TABLE IF EXISTS thematic_clusters;
DROP TABLE IF EXISTS project_regions;
DROP TABLE IF EXISTS regions;
DROP TABLE IF EXISTS project_organizations;
DROP TABLE IF EXISTS organizations;
DROP TABLE IF EXISTS projects;
DROP TABLE IF EXISTS solution_types;
DROP TABLE IF EXISTS funding_programs;
DROP TABLE IF EXISTS users;
DROP TABLE IF EXISTS user_roles;

-- ============================================================================
-- REFERENCE TABLES (Lookup tables)
-- ============================================================================

-- User Roles
CREATE TABLE user_roles (
    role_id INT AUTO_INCREMENT PRIMARY KEY,
    role_name VARCHAR(50) NOT NULL UNIQUE,
    role_description TEXT,
    can_view BOOLEAN DEFAULT TRUE,
    can_edit BOOLEAN DEFAULT FALSE,
    can_delete BOOLEAN DEFAULT FALSE,
    can_manage_users BOOLEAN DEFAULT FALSE,
    created_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
    updated_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;

-- Users Table
CREATE TABLE users (
    user_id INT AUTO_INCREMENT PRIMARY KEY,
    username VARCHAR(100) NOT NULL UNIQUE,
    email VARCHAR(255) NOT NULL UNIQUE,
    password_hash VARCHAR(255) NOT NULL,
    full_name VARCHAR(255),
    organization VARCHAR(255),
    role_id INT NOT NULL,
    is_active BOOLEAN DEFAULT TRUE,
    last_login TIMESTAMP NULL,
    created_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
    updated_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
    FOREIGN KEY (role_id) REFERENCES user_roles(role_id),
    INDEX idx_username (username),
    INDEX idx_email (email),
    INDEX idx_role (role_id)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;

-- Funding Programs
CREATE TABLE funding_programs (
    program_id INT AUTO_INCREMENT PRIMARY KEY,
    program_code VARCHAR(50) NOT NULL UNIQUE,
    program_name VARCHAR(255) NOT NULL,
    program_description TEXT,
    start_year YEAR,
    end_year YEAR,
    is_active BOOLEAN DEFAULT TRUE,
    created_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
    updated_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
    INDEX idx_code (program_code)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;

-- Solution Types
CREATE TABLE solution_types (
    solution_type_id INT AUTO_INCREMENT PRIMARY KEY,
    type_name VARCHAR(100) NOT NULL UNIQUE,
    type_description TEXT,
    type_category ENUM('Technology', 'Approach', 'Sector', 'Other') DEFAULT 'Other',
    is_active BOOLEAN DEFAULT TRUE,
    created_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
    updated_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
    INDEX idx_category (type_category)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;

-- Regions (Mediterranean countries and sub-regions)
CREATE TABLE regions (
    region_id INT AUTO_INCREMENT PRIMARY KEY,
    region_name VARCHAR(255) NOT NULL,
    region_code VARCHAR(10),
    country VARCHAR(100),
    region_type ENUM('Country', 'Sea Basin', 'Sub-region', 'Coastal Zone') DEFAULT 'Country',
    parent_region_id INT NULL,
    latitude DECIMAL(10, 7) NULL,
    longitude DECIMAL(10, 7) NULL,
    is_active BOOLEAN DEFAULT TRUE,
    created_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
    updated_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
    FOREIGN KEY (parent_region_id) REFERENCES regions(region_id) ON DELETE SET NULL,
    INDEX idx_country (country),
    INDEX idx_region_type (region_type),
    INDEX idx_parent (parent_region_id)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;

-- Thematic Clusters (KPI 4.1: at least 5 clusters)
CREATE TABLE thematic_clusters (
    cluster_id INT AUTO_INCREMENT PRIMARY KEY,
    cluster_name VARCHAR(255) NOT NULL UNIQUE,
    cluster_description TEXT,
    cluster_color VARCHAR(7), -- Hex color for visualization #RRGGBB
    cluster_icon VARCHAR(50), -- Icon identifier for UI
    mission_alignment TEXT, -- How this cluster aligns with Mission Ocean objectives
    is_active BOOLEAN DEFAULT TRUE,
    created_by INT,
    created_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
    updated_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
    FOREIGN KEY (created_by) REFERENCES users(user_id) ON DELETE SET NULL,
    INDEX idx_name (cluster_name)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;

-- Organizations (Partner organizations)
CREATE TABLE organizations (
    organization_id INT AUTO_INCREMENT PRIMARY KEY,
    organization_name VARCHAR(500) NOT NULL,
    organization_acronym VARCHAR(50),
    organization_type ENUM('Academic', 'Research', 'NGO', 'Government', 'Private', 'International', 'Community', 'Other') DEFAULT 'Other',
    country VARCHAR(100),
    city VARCHAR(255),
    website VARCHAR(500),
    contact_email VARCHAR(255),
    is_consortium_partner BOOLEAN DEFAULT FALSE,
    created_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
    updated_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
    INDEX idx_name (organization_name(100)),
    INDEX idx_acronym (organization_acronym),
    INDEX idx_country (country),
    INDEX idx_type (organization_type)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;

-- Keywords/Tags
CREATE TABLE keywords (
    keyword_id INT AUTO_INCREMENT PRIMARY KEY,
    keyword_text VARCHAR(255) NOT NULL UNIQUE,
    keyword_category VARCHAR(100), -- Optional grouping
    usage_count INT DEFAULT 0,
    is_approved BOOLEAN DEFAULT TRUE, -- For moderation if needed
    created_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
    updated_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
    INDEX idx_keyword (keyword_text),
    INDEX idx_category (keyword_category),
    INDEX idx_usage (usage_count)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;

-- ============================================================================
-- MAIN PROJECTS TABLE
-- ============================================================================

CREATE TABLE projects (
    project_id INT AUTO_INCREMENT PRIMARY KEY,
    
    -- Basic Information
    project_title VARCHAR(1000) NOT NULL,
    project_acronym VARCHAR(100),
    grant_agreement_number VARCHAR(50) UNIQUE,
    
    -- Program & Funding
    funding_program_id INT,
    total_budget DECIMAL(15, 2),
    currency VARCHAR(3) DEFAULT 'EUR',
    
    -- Timeline
    start_date DATE,
    end_date DATE,
    duration_months INT,
    project_status ENUM('Planned', 'Active', 'Completed', 'Suspended', 'Closed') DEFAULT 'Active',
    
    -- Description & Content
    abstract TEXT,
    objectives TEXT,
    expected_outcomes TEXT,
    
    -- Solution Classification
    solution_type_id INT,
    
    -- Coordinator
    coordinator_organization_id INT,
    coordinator_contact_name VARCHAR(255),
    coordinator_contact_email VARCHAR(255),
    
    -- Links & Resources
    website VARCHAR(500),
    cordis_link VARCHAR(500),
    documentation_url VARCHAR(500),
    
    -- Mission Alignment
    mission_alignment TEXT,
    eu_directive_alignment SET('Birds', 'Habitats', 'MSFD', 'WFD', 'Other'),
    
    -- Community-led indicators
    is_community_led BOOLEAN DEFAULT FALSE,
    community_description TEXT,
    
    -- Deployment potential
    deployment_stage ENUM('Concept', 'Research', 'Pilot', 'Demonstration', 'Deployed', 'Scaled') DEFAULT 'Research',
    replication_potential ENUM('Low', 'Medium', 'High', 'Very High') DEFAULT 'Medium',
    
    -- Data Source
    data_source VARCHAR(100) DEFAULT 'Portfolio Analysis 2023', -- Original source of data
    verified BOOLEAN DEFAULT FALSE,
    verified_by INT NULL,
    verified_date TIMESTAMP NULL,
    
    -- Metadata
    created_by INT,
    created_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
    updated_by INT,
    updated_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
    
    -- Foreign Keys
    FOREIGN KEY (funding_program_id) REFERENCES funding_programs(program_id) ON DELETE SET NULL,
    FOREIGN KEY (solution_type_id) REFERENCES solution_types(solution_type_id) ON DELETE SET NULL,
    FOREIGN KEY (coordinator_organization_id) REFERENCES organizations(organization_id) ON DELETE SET NULL,
    FOREIGN KEY (created_by) REFERENCES users(user_id) ON DELETE SET NULL,
    FOREIGN KEY (updated_by) REFERENCES users(user_id) ON DELETE SET NULL,
    FOREIGN KEY (verified_by) REFERENCES users(user_id) ON DELETE SET NULL,
    
    -- Indexes for search performance
    INDEX idx_title (project_title(100)),
    INDEX idx_acronym (project_acronym),
    INDEX idx_grant (grant_agreement_number),
    INDEX idx_status (project_status),
    INDEX idx_dates (start_date, end_date),
    INDEX idx_program (funding_program_id),
    INDEX idx_solution_type (solution_type_id),
    INDEX idx_coordinator (coordinator_organization_id),
    INDEX idx_community_led (is_community_led),
    INDEX idx_deployment (deployment_stage),
    FULLTEXT idx_fulltext (project_title, abstract, objectives)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;

-- ============================================================================
-- JUNCTION TABLES (Many-to-Many Relationships)
-- ============================================================================

-- Projects <-> Organizations (Many partner organizations per project)
CREATE TABLE project_organizations (
    project_org_id INT AUTO_INCREMENT PRIMARY KEY,
    project_id INT NOT NULL,
    organization_id INT NOT NULL,
    role_in_project ENUM('Coordinator', 'Partner', 'Associated Partner', 'Subcontractor', 'Other') DEFAULT 'Partner',
    contribution_description TEXT,
    created_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
    FOREIGN KEY (project_id) REFERENCES projects(project_id) ON DELETE CASCADE,
    FOREIGN KEY (organization_id) REFERENCES organizations(organization_id) ON DELETE CASCADE,
    UNIQUE KEY unique_project_org (project_id, organization_id),
    INDEX idx_project (project_id),
    INDEX idx_organization (organization_id),
    INDEX idx_role (role_in_project)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;

-- Projects <-> Regions (Projects can operate in multiple regions)
CREATE TABLE project_regions (
    project_region_id INT AUTO_INCREMENT PRIMARY KEY,
    project_id INT NOT NULL,
    region_id INT NOT NULL,
    is_primary_region BOOLEAN DEFAULT FALSE,
    coverage_description TEXT,
    created_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
    FOREIGN KEY (project_id) REFERENCES projects(project_id) ON DELETE CASCADE,
    FOREIGN KEY (region_id) REFERENCES regions(region_id) ON DELETE CASCADE,
    UNIQUE KEY unique_project_region (project_id, region_id),
    INDEX idx_project (project_id),
    INDEX idx_region (region_id),
    INDEX idx_primary (is_primary_region)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;

-- Projects <-> Thematic Clusters
CREATE TABLE project_clusters (
    project_cluster_id INT AUTO_INCREMENT PRIMARY KEY,
    project_id INT NOT NULL,
    cluster_id INT NOT NULL,
    is_primary_cluster BOOLEAN DEFAULT FALSE,
    relevance_score DECIMAL(3, 2) DEFAULT 1.00, -- 0.00 to 1.00
    assignment_rationale TEXT,
    assigned_by INT,
    assigned_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
    FOREIGN KEY (project_id) REFERENCES projects(project_id) ON DELETE CASCADE,
    FOREIGN KEY (cluster_id) REFERENCES thematic_clusters(cluster_id) ON DELETE CASCADE,
    FOREIGN KEY (assigned_by) REFERENCES users(user_id) ON DELETE SET NULL,
    UNIQUE KEY unique_project_cluster (project_id, cluster_id),
    INDEX idx_project (project_id),
    INDEX idx_cluster (cluster_id),
    INDEX idx_primary (is_primary_cluster),
    INDEX idx_relevance (relevance_score)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;

-- Projects <-> Keywords
CREATE TABLE project_keywords (
    project_keyword_id INT AUTO_INCREMENT PRIMARY KEY,
    project_id INT NOT NULL,
    keyword_id INT NOT NULL,
    source ENUM('Original', 'Manual', 'Auto-Generated', 'Community-Added') DEFAULT 'Manual',
    created_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
    FOREIGN KEY (project_id) REFERENCES projects(project_id) ON DELETE CASCADE,
    FOREIGN KEY (keyword_id) REFERENCES keywords(keyword_id) ON DELETE CASCADE,
    UNIQUE KEY unique_project_keyword (project_id, keyword_id),
    INDEX idx_project (project_id),
    INDEX idx_keyword (keyword_id),
    INDEX idx_source (source)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;

-- ============================================================================
-- SYNERGIES TABLE (KPI 4.1: At least 10 synergies documented)
-- ============================================================================

CREATE TABLE synergies (
    synergy_id INT AUTO_INCREMENT PRIMARY KEY,
    synergy_title VARCHAR(500) NOT NULL,
    synergy_type ENUM(
        'Shared Methodology',
        'Complementary Goals',
        'Resource Sharing',
        'Geographic Overlap',
        'Technology Transfer',
        'Knowledge Exchange',
        'Joint Activities',
        'Other'
    ) DEFAULT 'Other',
    description TEXT NOT NULL,
    potential_impact TEXT,
    synergy_status ENUM('Identified', 'Under Review', 'Confirmed', 'Active', 'Completed', 'Discontinued') DEFAULT 'Identified',
    
    -- Who identified and when
    identified_by INT,
    identified_by_organization INT,
    identified_date DATE,
    
    -- Follow-up information
    contact_person_name VARCHAR(255),
    contact_person_email VARCHAR(255),
    action_items TEXT,
    notes TEXT,
    
    -- Metadata
    created_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
    updated_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
    
    FOREIGN KEY (identified_by) REFERENCES users(user_id) ON DELETE SET NULL,
    FOREIGN KEY (identified_by_organization) REFERENCES organizations(organization_id) ON DELETE SET NULL,
    INDEX idx_type (synergy_type),
    INDEX idx_status (synergy_status),
    INDEX idx_date (identified_date)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;

-- Synergies <-> Projects (A synergy can link 2 or more projects)
CREATE TABLE synergy_projects (
    synergy_project_id INT AUTO_INCREMENT PRIMARY KEY,
    synergy_id INT NOT NULL,
    project_id INT NOT NULL,
    role_description TEXT, -- How this project contributes to the synergy
    created_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
    FOREIGN KEY (synergy_id) REFERENCES synergies(synergy_id) ON DELETE CASCADE,
    FOREIGN KEY (project_id) REFERENCES projects(project_id) ON DELETE CASCADE,
    UNIQUE KEY unique_synergy_project (synergy_id, project_id),
    INDEX idx_synergy (synergy_id),
    INDEX idx_project (project_id)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;

-- ============================================================================
-- SEARCH & ANALYTICS
-- ============================================================================

-- Search Logs (Track dashboard usage for analytics)
CREATE TABLE search_logs (
    log_id INT AUTO_INCREMENT PRIMARY KEY,
    user_id INT NULL,
    session_id VARCHAR(100),
    search_type ENUM('Keyword', 'Region', 'Cluster', 'Organization', 'Solution Type', 'Advanced', 'Full Text') NOT NULL,
    search_query TEXT,
    filters_applied JSON, -- Store complex filter combinations
    results_count INT,
    user_ip VARCHAR(45),
    user_agent VARCHAR(500),
    search_timestamp TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
    FOREIGN KEY (user_id) REFERENCES users(user_id) ON DELETE SET NULL,
    INDEX idx_timestamp (search_timestamp),
    INDEX idx_user (user_id),
    INDEX idx_type (search_type)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;

